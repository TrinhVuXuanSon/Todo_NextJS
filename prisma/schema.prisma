generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id @default(uuid())
  username  String   @unique
  password  String
  todos     Todos[]
}

model Todos {
  id        String   @id @default(uuid())
  name      String
  completed Boolean  @default(false)
  userId    String
  user      User     @relation(fields: [userId], references: [id])
}